<?xml version="1.0" encoding="us-ascii"?>
<Loqui xmlns="http://tempuri.org/LoquiSource.xsd">
  <Object name="GameplayOptions" recordType="GPOF" objType="Record" baseClass="StarfieldMajorRecord">
    <Fields>
      <String name="Name" recordType="NNAM" translated="Normal" />
      <String name="Description" recordType="DNAM" translated="Normal" />
      <RefDirect name="Data" refName="AGameplayOptionsData" recordType="TNAM" binary="Custom" />
      <RefDirect name="GameplayOptionDataRange" refName="GameplayOptionDataRange" />
      <RefList name="Rewards" refName="GameplayOptionReward" />
      <List name="Keywords" counterRecType="KSIZ" recordType="KWDA">
        <FormLink refName="Keyword" />
      </List>
    </Fields>
  </Object>
  <Object name="AGameplayOptionsData" objType="Subrecord" />
  <Object name="BoolGameplayOptionsData" objType="Subrecord" baseClass="AGameplayOptionsData" >
    <Fields>
      <Bool name="Value1" byteLength="1" recordType="VNAM" />
      <Bool name="Value2" byteLength="1" recordType="WNAM" />
    </Fields>
  </Object>
  <Object name="FloatGameplayOptionsData" objType="Subrecord" baseClass="AGameplayOptionsData" >
    <Fields>
      <Float name="Value1" byteLength="1" recordType="VNAM" />
      <Float name="Value2" byteLength="1" recordType="WNAM" />
    </Fields>
  </Object>
  <Object name="GameplayOptionDataRange" recordType="GPOD" objType="Subrecord">
    <Fields>
      <Float name="Min" />
      <Float name="Max" />
      <Float name="Step" />
    </Fields>
  </Object>
  <Object name="GameplayOptionReward" objType="Subrecord">
    <Fields>
      <String name="Description" recordType="VOVS" translated="Normal" />
      <Float name="SettingValue" recordType="VORV" />
      <String name="Name" recordType="RESN" translated="Normal" />
      <Float name="RewardValue" recordType="VORN" />
    </Fields>
  </Object>
</Loqui>
