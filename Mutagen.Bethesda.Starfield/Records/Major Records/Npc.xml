        <Ref>
          <Direct refName="ObjectTemplate">
            <GenericSpecification Definition="Npc.Property" TypeToSpecify="T" />
          </Direct>
        </Ref>
      </List>
      <Marker name="STOPMarker" recordType="STOP" />
