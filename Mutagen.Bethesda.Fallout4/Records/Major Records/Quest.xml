      <Int32 name="AliasID" recordType="ALEA" />
    </Fields>
  </Object>
  <Object name="CreateReferenceToObject" objType="Subrecord">
    <Fields>
      <FormLink name="Object" refName="Fallout4MajorRecord" recordType="ALCO" nullable="False" />
